server:
  listen: 0.0.0.0:8080
  root: /home/seriots/42_cursus/Server/test_folder
  server_name: myname
  index: index.php
  permissions: 7
  max_body_size: 32000000
  autoindex: off
  error_pages:
    404: /home/seriots/42_cursus/Server/test_error/404.html
    405: /home/seriots/42_cursus/Server/test_error/405.html
  cgi:
    php: /usr/bin/php-cgi
  location: /
    post_dir: /home/seriots/42_cursus/Server/test_folder/upload
    max_body_size: 32000000
    
  location: /oui
    default_file: /home/seriots/42_cursus/Server/test_folder/oui/ANon.html
    post_dir: /home/seriots/42_cursus/Server/test_folder/upload
    root: /home/seriots/42_cursus/Server/test_folder/
    index: index.php
    permissions: 7
    max_body_size: 4096
    autoindex: off
  location: /upload
    root: /home/seriots/42_cursus/Server/test_folder/upload
    autoindex: on
  location: /upload/ici/
    redirect: http://0.0.0.0:8080/upload/
# this is a comment
  # it can be shifted by any space.
# mandatory: listen, root, index
server:
# listen can take either port only, or a host witch can be resolved in IPV4
  listen: localhost:8080
  root: /home/seriots/42_cursus/Server/test_folder
  server_name: myname
  index: index.php
  permissions: 7
  max_body_size: 32000000
  autoindex: off
  error_pages:
    404: /home/seriots/42_cursus/Server/test_error/404.html
    405: /home/seriots/42_cursus/Server/test_error/405.html
  location: /
    post_dir: /home/seriots/42_cursus/Server/test_folder/upload
    max_body_size: 32000000
    
  location: /oui
    default_file: /home/seriots/42_cursus/Server/test_folder/oui/ANon.html
    post_dir: /home/seriots/42_cursus/Server/test_folder/upload
    root: /home/seriots/42_cursus/Server/test_folder/
    index: index.php
    permissions: 7
    max_body_size: 4096
    autoindex: off
#   cgi:
#      .py: /bin/python3
#      .php: /usr/bin/php-cgi
#      lama: /coucou/sh
  location: /upload
    root: /home/seriots/42_cursus/Server/test_folder/upload
    autoindex: on
  location: /upload/ici/
    redirect: http://localhost:8080/upload/