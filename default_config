server:
  listen: 0.0.0.0:8080
  root: /mnt/nfs/homes/fmauguin/Documents/webserv/pages/www
  server_name: Webserv
  index: index.html
  permissions: 7
  max_body_size: 32000000
  autoindex: off
  error_pages:
    404: /mnt/nfs/homes/fmauguin/Documents/webserv/pages/error/404.html
    405: /mnt/nfs/homes/fmauguin/Documents/webserv/pages/error/405.html
  location: /
    post_dir: /mnt/nfs/homes/fmauguin/Documents/webserv/pages/www/upload
    max_body_size: 32000000
    cgi:
      pl: /bin/perl
      perl: /bin/perl
      php: /bin/php-cgi
      py: /bin/python3
    
  location: /ou
    default_file: /mnt/nfs/homes/fmauguin/Documents/webserv/pages/www/oui/ANon.html
    post_dir: /mnt/nfs/homes/fmauguin/Documents/webserv/pages/www/upload
    root: /mnt/nfs/homes/fmauguin/Documents/webserv/pages/www/
    index: test.php
    permissions: 7
    max_body_size: 4096
    autoindex: off
  location: /upload
    root: /mnt/nfs/homes/fmauguin/Documents/webserv/pages/www/upload
    autoindex: on
  location: /upload/ici/
    redirect: http://0.0.0.0:8080/upload/

